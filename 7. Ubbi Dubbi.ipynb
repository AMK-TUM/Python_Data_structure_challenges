    "def ubbi_dubbi(word):\n",
    "    \n",
    "    vowels = ['a','e','i','o','u']\n",
    "    new = []\n",
    "    for i in word:\n",
    "        \n",
    "        if i in vowels:\n",
    "            new.append(f'ub{i}')\n",
    "        else:\n",
    "            new.append(f'{i}') \n",
    "            \n",
    "    return ''.join(new)"
   ]
  },
